<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--Mapeo de la clase Alumno con la tabla alumnos-->
<hibernate-mapping>
	<class name="clases.Users" table="users" catalog="moliGest">
        <id name="id">
            <column name="id"/>
            <generator class="increment"/>
        </id>
        <property name="name"/>
        <property name="apellido1"/>
        <property name="apellido2"/>
        <property name="nif"/>
        <property name="password"/>
        <property name="role"/>
        <property name="email"/>

        <!-- RelaciÃ³n muchos a uno con Socios -->
        <many-to-one name="socio" class="clases.Socios" column="idSocio" not-null="true"/>

        
    </class>
</hibernate-mapping>