<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--Mapeo de la clase Alumno con la tabla alumnos-->
<hibernate-mapping>
    <class name="clases.LineasFacturas" table="lineasFacturas" catalog="moliGest">
        <id name="id">
			<column name="id" />
			<generator class="increment" />
        </id>
		<property name="idFactura" />
		<property name="idProducto" />
		<property name="cantidad" />
		<property name="precio" />
		<property name="iva" />
		<property name="descuento" />
		<property name="total" />
     </class>
</hibernate-mapping>
