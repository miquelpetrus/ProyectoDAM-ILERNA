<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--Mapeo de la clase Alumno con la tabla alumnos-->
<hibernate-mapping>
    <class name="clases.Terceros" table="terceros" catalog="moliGest">
        <id name="id">
			<column name="id" />
			<generator class="increment" />
        </id>
        <property name="nombre" />
        <property name="cif" column="cif" unique="true" length="10"/>
        <property name="direccion" />
        <property name="poblacion" />
        <property name="cp" />
        <property name="provincia" />
        <property name="telefono" />
        <property name="email" column="email" unique="true"/>
        <property name="contacto" />
     </class>
 </hibernate-mapping>
        