<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!--Mapeo de la clase Alumno con la tabla alumnos-->
<hibernate-mapping>
    <class name="clases.Proveedores" table="proveedores" catalog="tesGiloM">
        <id name="id">
			<column name="id" />
			<generator class="increment" />
        </id>
        <property name="nombre" />
        <property name="cif" />
        <property name="direccion" />
        <property name="poblacion" />
        <property name="cp" />
        <property name="provincia" />
        <property name="telefono" />
        <property name="email" />
        <property name="web" />
        <property name="contacto" />
     </class>
 </hibernate-mapping>
        